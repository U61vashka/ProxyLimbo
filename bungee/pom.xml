            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>net.md-5</groupId>
            <artifactId>bungeecord-api</artifactId>
            <version>${bungeecord.version}</version>
            <type>javadoc</type>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.github.SpigotMC.BungeeCord</groupId>
            <artifactId>bungeecord-proxy</artifactId>
            <version>${bungeecord.proxy.version}</version>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>com.github.U61vashka.ConfigurationLibrary</groupId>
            <artifactId>core</artifactId>
            <version>${configuration.library.version}</version>
        </dependency>
        <dependency>
            <groupId>com.github.U61vashka.ConfigurationLibrary</groupId>
            <artifactId>bungee</artifactId>
            <version>${configuration.library.version}</version>
        </dependency>

        <dependency>
            <groupId>com.github.Revxrsal.Lamp</groupId>
            <artifactId>common</artifactId>
            <version>${lamp.version}</version>
        </dependency>
        <dependency>
            <groupId>com.github.Revxrsal.Lamp</groupId>
            <artifactId>bungee</artifactId>
            <version>${lamp.version}</version>
        </dependency>

        <dependency>
            <groupId>com.ubivashka.limbo.ProxyLimbo</groupId>
            <artifactId>core</artifactId>
            <version>${core.version}</version>
        </dependency>
